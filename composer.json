{
    "name": "codefog/contao-cloudflare-turnstile",
    "description": "Cloudflare Turnstile extension for Contao Open Source CMS",
    "keywords": ["contao", "cloudflare", "turnstile", "captcha", "spam", "bo"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "symfony/http-client": "^6.4 || 7.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "terminal42/contao-build-tools": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\CloudflareTurnstileBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\CloudflareTurnstileBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "terminal42/contao-build-tools": true,
            "php-http/discovery": true
        }
    }
}
